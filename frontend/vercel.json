{
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://localhost:8000/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "http://localhost:8000"
  }
} 